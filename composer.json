{
    "name": "erik/pluginui",
    "description": "A plugin",
    "license": "GPL-3.0-or-later",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "Erik Golinelli",
            "email": "erik@codekraft.it",
            "homepage": "https://author-url.test"
        }
    ],
    "homepage": "https://github.com/WPBP/WordPress-Plugin-Boilerplate-Powered",
    "require": {
        "php": ">=7.3",
        "inpsyde/wp-context": "1.3.*"
    },
    "require-dev": {
        "yoast/i18n-module": "3.1.*",
        "friendsofphp/php-cs-fixer": "3.*",
        "squizlabs/php_codesniffer": "^3",
        "phpmd/phpmd" : "@stable",
        "phpstan/phpstan": "^1.8",
        "vimeo/psalm": "^4.29",
        "phpcompatibility/php-compatibility": "*",
        "wp-coding-standards/wpcs": "*",
        "automattic/vipwpcs": "^2.3",
        "phpcompatibility/phpcompatibility-wp": "*",
        "slevomat/coding-standard": "^7.2",
        "phpstan/extension-installer": "^1.2",
        "szepeviktor/phpstan-wordpress": "^1.1",
        "ergebnis/composer-normalize": "^2.6",
        "composer/installers": "1.x"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "pluginui\\Backend\\": "backend/",
            "pluginui\\Frontend\\": "frontend/",
            "pluginui\\Rest\\": "rest/",
            "pluginui\\Engine\\": "engine/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "phpro/grumphp": true,
            "phpstan/extension-installer": true
        }
    }
}
